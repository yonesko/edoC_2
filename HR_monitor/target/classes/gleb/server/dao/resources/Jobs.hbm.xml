<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gleb.server.model.Job" table="JOBS_TEST">
        <meta attribute="class-description">
            This class contains the employee detail.
        </meta>
        <id name="id" type="long" column="JOB_ID">
            <generator class="native"/>
        </id>
        <property name="title" column="JOB_TITLE" type="string"/>
        <property name="minSalary" column="MIN_SALARY" type="int"/>
        <property name="maxSalary" column="MAX_SALARY" type="int"/>
    </class>
</hibernate-mapping>